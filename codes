//ENTC B1
#include <iostream>
using namespace std;
class Myclass {
    private:
    int value;
    public:
    Myclass(int val) {
        value=val;
        cout<<"Your entered value is: "<<value<<endl;
    }
    Myclass(const Myclass&obj) {
        value=obj.value;
        cout<<"Copy constructor called! Value: "<<value<<endl;
    }
};

int main() {
   int a;
   cout<<"Enter a value: ";
   cin>>a;
   Myclass obj1(a); 
   Myclass obj2=obj1;
   return 0;
}


//Rhea kulkarni
//23070123106
//ENTC B1
#include <iostream>
using namespace std;
class Myclass {
    private:
    int value1,value2;
    public:
    Myclass(int val1=0, int val2=0) {
        value1=val1;
        value2=val2;
        cout<<"Constructor called! Your entered value is: "<<value1<<" and "<<value2 <<endl;
    }   
};
int main() {
   
   Myclass obj1; 
   Myclass obj2(2);
   Myclass obj3(3,5);
   return 0;
}


